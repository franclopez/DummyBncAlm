<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="co.com.tcs.bancolombia.bancaalamano" version="0.0.1">
  <name>Ahorro a la Mano</name>
  <description>
       Bancolombia Ahorro a la Mano
    </description>
  <author email="hi@ionicframework" href="http://ionicframework.com/">
        Ionic Framework Team
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="SplashScreen" value="splash"/>
  <preference name="SplashScreenDelay" value="10000"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="BackupWebStorage" value="none"/>
  
  <!--
  <icon src="img/ic_mdpi.png"/>
  <icon src="ic_ldpi.png" gap:platform="android" gap:density="ldpi"/>
  <icon src="ic_mdpi.png" gap:platform="android" gap:density="mdpi"/>
  <icon src="ic_hdpi.png" gap:platform="android" gap:density="hdpi"/>
  -->
  <icon src="res/icons/android/ic_ldpi.png" platform="android" density="ldpi"/>
  <icon src="res/icons/android/ic_mdpi.png" platform="android" density="mdpi"/>
  <icon src="res/icons/android/ic_hdpi.png" platform="android" density="hdpi"/>
  <!--
  <icon src="icon-60.png" gap:platform="ios" width="60" height="60"/>
  <icon src="icon-60@2x.png" gap:platform="ios" width="120" height="120"/>
  <icon src="icon.png" gap:platform="ios" width="57" height="57"/>
  <icon src="icon@2x.png" gap:platform="ios" width="114" height="114"/>
  -->
  <icon src="res/icons/ios/icon-60.png" platform="ios" width="60" height="60"/>
  <icon src="res/icons/ios/icon-60@2x.png" platform="ios" width="120" height="120"/>
  <icon src="res/icons/ios/icon.png" platform="ios" width="57" height="57"/>
  <icon src="res/icons/ios/icon@2x.png" platform="ios" width="114" height="114"/>
  
  <!--
  <gap:splash src="img/splash_mdpi.png"/>
  <gap:splash src="img/splash_mdpi.png" gap:platform="android" gap:qualifier="mdpi"/>
  <gap:splash src="img/splash_hdpi.png" gap:platform="android" gap:qualifier="hdpi"/>
  <gap:splash src="img/Default.png" gap:platform="ios" width="320" height="480"/>
  <gap:splash src="img/Default@2x.png" gap:platform="ios" width="640" height="960"/>
  <gap:splash src="img/Default-568h@2x.png" gap:platform="ios" width="640" height="1136"/>
  -->
  <cdv:splash src="res/screens/android/splash_mdpi.png"/>
  <cdv:splash src="res/screens/android/splash_mdpi.png" cdv:platform="android" cdv:qualifier="mdpi"/>
  <cdv:splash src="res/screens/android/splash_hdpi.png" cdv:platform="android" cdv:qualifier="hdpi"/>
  <cdv:splash src="res/screens/ios/Default.png" cdv:platform="ios" width="320" height="480"/>
  <cdv:splash src="res/screens/ios/Default@2x.png" cdv:platform="ios" width="640" height="960"/>
  <cdv:splash src="res/screens/ios/Default-568h@2x.png" cdv:platform="ios" width="640" height="1136"/>
  <!--
  <plugin name="InAppBrowser" value="org.apache.cordova.InAppBrowser"/>
  <plugin name="InAppBrowser" value="CDVInAppBrowser"/>
  <plugin name="SplashScreen" value="org.apache.cordova.SplashScreen"/>
  <plugin name="SplashScreen" value="CDVSplashScreen"/>
  -->
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
  <feature name="InAppBrowser">
    <param name="android-package" value="org.apache.cordova.InAppBrowser"/>
  </feature>
  <feature name="InAppBrowser">
    <param name="ios-package" value="CDVInAppBrowser"/>
  </feature>
  <feature name="SplashScreen">
    <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen"/>
  </feature>
  <feature name="SplashScreen">
    <param name="ios-package" value="CDVSplashScreen"/>
  </feature>
</widget>